.modal-holder
	position absolute
	width 400px
	margin 0 0 0 -200px
	top 30px
	left 50%
	z-index 99999

	.modal-container
		float left
		width 358px
		padding 26px 20px 36px 40px
		margin 0
		background $background-texture
		border $border-black
		border-radius 8px

		.fa-close
			float left
			width 18px
			padding 0
			margin 0 0 24px 160px
			font-weight 600
			font-size 18px
			color $grey
			text-align center
			cursor pointer

		h1
			float left
			width 338px
			padding 0
			margin 0
			font-style italic
			font-weight 500
			font-size 31px
			line-height 31px
			text-align center

		h2
			float left
			width 338px
			padding 0 0 8px 0
			margin 8px 0 30px 0
			font-weight 700
			font-size 45px
			line-height 45px
			border-bottom 6px solid #999
			text-align center

		.row
			float left
			width 338px
			padding 0
			margin 0

			input
				float left
				width 336px
				height 62px
				padding 6px 0 0 0
				margin 0 0 20px 0
				// font-style italic
				font-size 29px
				line-height 29px
				text-align center
				border $border

		.rx-card
			float left
			width 336px
			height 160px
			margin 0 0 22px 0
			background $card
			// border 1px solid #333
			border-bottom 2px solid $black
			border-radius 8px


		.submit
			float left
			width 338px
			height 68px
			padding 0
			margin 0
			font-weight 700
			font-style italic
			font-size 35px
			line-height 59px
			color $white
			background $burgundy-gradient
			border none
			outline none
			border-bottom 2px solid $black
			border-radius 8px
			box-shadow inset 0 1px 0 rgba(255,255,255,.1), 0 1px 2px rgba(0,0,0,.05);
			box-shadow 1px 1px 4px #aaa
			text-align center
			text-shadow $dark-shadow
			cursor pointer

