.page
	position relative
	width 100%
	padding 0
	margin 0 auto

	.content-holder
		position relative
		width 1000px
		margin 0 auto
		z-index 100

        .content
			float left
			width 1000px
			margin 0
			padding 0

			.main
				float left
				width 480px
				padding 0
				margin 60px 0 20px 0

				.rx-card
					float left
					width 414px
					padding 32px 0px 22px 44px
					margin 0 0 14px 0
					background $card
					border-bottom 4px solid #333
					border-radius 12px

					h2
						float left
						width 100%
						margin 0 0 12px 0
						font-weight 600
						font-size 39px
						line-height 39px
						text-align left

					.card-logo
						position absolute
						width 140px
						height 160px
						padding 0
						margin 30px 0 0 264px
						background none
						z-index 1

						img
							display none
							float left
							width 100%
							height 100%

					.card-content
						float left
						width 344px
						// height 190px
						padding 0
						margin 0
						z-index 10

						.card-headline
							float left
							width 100%

							.company
								float left
								width 100%
								margin 0 0 28px 0

								h5
									float left
									font-style italic
									font-weight 500
									font-size 21px
									line-height 25px
									color $black
									text-align left

								span
									float left
									margin 0 12px
									font-size 18px
									line-height 25px
									color $grey
									text-align left

							.patient-holder
								float left
								width 100%
								padding 0
								margin 0

								.row
									float left
									width 244px
									padding 0
									margin 0 0 12px 0

									input
										float left
										width 300px
										margin 0 0 18px 0
										font-weight 500
										font-size 27px
										line-height 27px
										color $grey-light
										background none
										border none
										outline none
										text-align left

									h6
										float left
										width 82px
										padding 0
										margin 0
										font-weight 700
										font-size 11px
										line-height 11px
										color $grey-light

				.rx-card-download
					float left
					width 478px
					padding 0
					margin 0 0 30px 36px

					.column
						float left
						width 86px
						padding 16px 0 12px 0
						margin 0 14px 0 0
						background $header-tile
						border-bottom 4px solid #333
						border-radius 8px
						cursor pointer

						p
							float left
							width 100%
							padding 0
							margin 0
							font-weight 500
							color $white
							font-size 18px
							line-height 18px
							text-align center
							text-shadow $shadow

							span.fa-cloud
							span.fa-mobile
							span.fa-send
							span.fa-file-text
								float left
								width 100%
								padding 0
								margin 0
								height 40px
								font-size 30px
								text-align center

							span.fa-mobile
								height 48px
								font-size 41px
								margin -8px 0 0 0

							span.fa-send
								font-size 25px

							span.fa-file-text
								font-size 26px

			.points
				float left
				width 470px
				height 406px
				padding 0 0 0 50px
				margin 60px 0 0 0
				// background $header-tile
				// border-bottom 4px solid #333
				// border-radius 8px

				.point
					float left
					width 100%
					padding 0
					margin 0 0 32px 0

					div.fa-certificate
						float left
						width 76px
						padding 0
						margin 0
						color #ff9
						font-size 49px
						text-shadow $shadow

					.text
						float left
						width 394px
						padding 0
						margin 0

						h3
							float left
							width 100%
							padding 0
							margin 0
							font-weight 600
							font-size 35px
							line-height 49px
							color $white
							text-align left
							text-shadow $shadow

							span
								padding 0 0 10px 0
								font-style italic
								border-bottom 6px solid #666

			.center
				float left
				width 1000px
				padding 0
				margin 0 0 40px 0

				span.fa-certificate
					position absolute
					width 30px
					margin -16px 0 0 878px
					font-size 39px
					color #ff9
					text-shadow $shadow

				h1
					float left
					width 800px
					height 60px
					padding 0
					margin 0 0 18px 100px
					font-weight 500
					font-size 29px
					line-height 62px
					color $white
					text-align center
					text-shadow $shadow
					background $header-tile
					border-bottom 4px solid #333
					border-radius 8px

				.center-holder
					float left
					width 958px
					padding 34px 0 10px 40px
					margin 0
					background #FFF
					border-bottom 4px solid #333
					border-radius 12px

					.logo
						float left
						padding 0 30px 38px 30px
						margin 0

						img
							float left
							height 40px
							padding 0
							margin 0
							font-size 18px

			.locator
				float left
				width 1000px
				margin 0
				padding 0

				h2
					float left
					width 100%
					padding 0
					margin 0 0 18px 0
					font-weight 500
					font-size 29px
					line-height 64px
					color $white
					text-align center
					text-shadow $shadow

				.submit
					float left
					width 120px
					height 52px
					font-size 23px
					line-height 49px
					color $white
					border none
					background $header-tile
					border-bottom 4px solid #333
					border-radius 8px
					cursor pointer

				.locator-zip
					float left
					width 300px
					padding 10px 40px 30px 40px
					margin 0 20px 0 0
					background $header-tile
					border-bottom 4px solid #333
					border-radius 12px

					input
						float left
						width 158px
						height 48px
						padding 0
						margin 0 20px 0 0
						font-size 23px
						font-weight 500
						line-height 23px
						border none
						border-bottom 4px solid #333
						border-radius 8px
						text-align center
						cursor pointer

				.locator-state
					float left
					width 520px
					padding 10px 40px 30px 40px
					margin 0 0 30px 0
					background $header-tile
					border-bottom 4px solid #333
					border-radius 12px

					select
						float left
						width 178px
						height 52px
						padding 0
						margin 0 20px 0 0
						font-weight 400
						font-size 23px
						line-height 23px
						background $white
						border none
						border-bottom 4px solid #333
						border-radius 8px
			            appearance:none
						-webkit-appearance none
						-moz-appearance none

				.locator-results
					float left
					width 920px
					background $header-tile
					padding 40px 40px 35px 40px
					border-radius 12px
					border-bottom 4px solid #333

					h2
						float left
						width 100%
						padding 0
						margin 0 0 28px 0
						font-weight 500
						font-size 39px
						line-height 39px
						color $white
						text-align center
						text-shadow $shadow

					.locator-results-content
						float left
						width 920px
						padding 0
						background $white

						.row
							float left
							width 100%
							height 44px
							border-bottom 1px solid #ccc

							h4
								float left
								width 100%
								font-weight 600
								font-size 19px
								line-height 46px
								color $grey

							h4.title
								font-size 20px
								color $black

							.pharmacy
								float left
								width 250px
								height 44px
								padding 0 0 0 20px
								border-right 1px solid #ccc

							.address
								float left
								width 270px
								height 44px
								padding 0 0 0 20px
								border-right 1px solid #ccc

							.phone
								float left
								width 179px
								height 44px
								padding 0
								border-right 1px solid #ccc

								h4
									text-align center

							.directions
								float left
								width 168px
								height 44px
								padding 0

								h4
									text-align center

			.testimonals-holder
				float left
				width 1020px
				padding 0
				margin 0

				.testimonals
					float left
					width 1020px
					padding 0
					margin 50px 0 40px 0

					.testimonal
						float left
						width 240px
						padding 28px 40px 28px 40px
						margin 0 20px 20px 0
						background $card
						border-bottom 4px solid #333
						border-radius 12px

						p
							float left
							width 100%
							padding 0
							margin 0 0 24px 0
							font-style italic
							font-weight 600
							font-size 19px
							line-height 31px
							color $black
							text-align center
							text-shadow $white-shadow
						h2
							float left
							width 100%
							padding 0
							font-weight 600
							font-size 23px
							line-height 23px
							margin 0 0 12px 0
							text-align center
							text-shadow $white-shadow

						h3
							float left
							width 100%
							padding 0
							margin 0
							font-weight 500
							font-size 17px
							line-height 17px
							text-align center
							text-shadow $white-shadow

					.testimonal.hide
						display block
