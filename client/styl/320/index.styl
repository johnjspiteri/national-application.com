.page
	position relative
	width 320px
	padding 0
	margin 0 auto

    .content
		float left
		width 320px
		margin 0
		padding 0

		.main
			float left
			width 280px
			padding 0
			margin 0 0 0 20px

			.card
				float left
				width 254px
				padding 20px 0px 12px 26px
				margin 0 0 12px 0
				background $card
				border-top none
				border-bottom 4px solid #333
				border-radius 12px

				h2
					float left
					width 100%
					margin 0 0 8px 0
					font-weight 600
					font-size 29px
					line-height 35px
					text-align left
				//
				// .card-logo
				// 	position absolute
				// 	width 100px
				// 	height 160px
				// 	padding 0
				// 	margin 30px 0 0 140px
				// 	background none
				// 	z-index 1
				//
				// 	img
				// 		display none
				// 		float left
				// 		width 100%
				// 		height 100%

				.card-content
					float left
					width 250px
					padding 0
					margin 0

					.company
						float left
						width 100%
						margin 0 0 16px 0

						h5
							float left
							font-style italic
							font-weight 500
							font-size 19px
							line-height 22px
							color $black
							text-align left
							text-shadow $white-shadow

						span
							float left
							margin 0 8px
							font-size 17px
							line-height 24px
							color $grey
							text-align left

					.patient-holder
						float left
						width 100%
						padding 0
						margin 0

						.row
							float left
							width 100%
							padding 0
							margin 0 0 9px 0

							input
								float left
								width 100%
								margin 0 0 12px 0
								font-weight 500
								font-size 18px
								line-height 24px
								color $grey-light
								background none
								border none
								outline none
								text-align left
								text-shadow $white-shadow

							h6
								float left
								width 72px
								padding 0
								margin 0
								font-weight 700
								font-size 9px
								line-height 9px
								color $grey-light
								text-shadow $white-shadow

			.download
				float left
				width 300px
				padding 0
				margin 0 0 38px 0

				.tag
					float left
					width 61px
					padding 10px 0 10px 0
					margin 0 12px 0 0
					background $header-tile
					border-bottom 4px solid #333
					border-radius 8px
					cursor pointer

					p
						float left
						width 100%
						padding 0
						margin 2px 0 0 0
						font-weight 500
						color $white
						font-size 16px
						line-height 16px
						text-align center
						text-shadow $shadow

					.fa-cloud
					.fa-mobile
					.fa-send
					.fa-file-text
						float left
						width 100%
						padding 0
						margin 0
						height 26px
						font-size 21px
						line-height 21px
						color $white
						text-align center
						text-shadow $shadow

					.fa-mobile
						font-size 28px

					.fa-send
						font-size 16px

					.fa-file-text
						font-size 19px

		.points
			float left
			width 280px
			padding 0 0 0 0
			margin 0 0 10px 20px

			.point
				float left
				width 100%
				padding 0
				margin 0

				.fa-certificate
					float left
					width 52px
					padding 0
					margin 0 0 0 4px
					color #ff9
					font-size 39px
					text-shadow $shadow

				.text
					float left
					width 224px
					padding 0
					margin 3px 0 32px 0

					h3
						float left
						width 100%
						padding 0
						margin 0
						font-weight 600
						font-size 25px
						line-height 35px
						color $white
						text-align left
						text-shadow $shadow

						span
							padding 0 0 10px 0
							font-style italic
							border-bottom 6px solid #666

		.center
			float left
			width 100%
			padding 0
			margin 0 0 20px 0

			.fa-certificate
				position absolute
				width 20px
				margin -18px 0 0 270px
				font-size 39px
				color #ff9
				text-shadow $shadow

			h1
				float left
				width 220px
				padding 14px 20px
				margin 0 0 18px 30px
				font-weight 500
				font-size 27px
				line-height 35px
				color $white
				text-align center
				text-shadow $shadow
				background $header-tile
				border-bottom 4px solid #333
				border-radius 8px

			.center-holder
				float left
				width 300px
				padding 20px 0 24px 20px
				margin 0
				background #FFF
				border-left none
				border-right none
				border-bottom 4px solid #333

				.logo
					float left
					padding 0 20px 20px 0
					margin 0 0 0 10px

					img
						float left
						height 30px
						padding 0
						margin 0
						font-size 22px

		.locator
			float left
			width 280px
			margin 0 0 0 20px
			padding 0

			.locator-zip
			.locator-state
				float left
				width 240px
				padding 20px 20px 20px 20px
				margin 0 0 20px 0
				background $header-tile
				border-bottom 4px solid #333
				border-radius 12px

				h2
					float left
					width 100%
					padding 0
					margin 0 0 18px 0
					font-weight 500
					font-size 24px
					line-height 35px
					color $white
					text-align center
					text-shadow $shadow

				input
					float left
					width 200px
					height 48px
					padding 0
					margin 0 0 14px 20px
					font-size 18px
					font-weight 500
					line-height 23px
					border none
					border-bottom 4px solid #333
					border-radius 8px
					text-align center
					cursor pointer

				select
					float left
					width 200px
					height 52px
					padding 0
					margin 0 0 10px 20px
					font-weight 400
					font-size 23px
					line-height 23px
					background $white
					border none
					border-bottom 4px solid #333
					border-radius 8px
		            appearance:none
					-webkit-appearance none
					-moz-appearance none

				.submit
					float left
					width 200px
					margin 0 0 0 20px
					height 48px
					font-size 23px
					line-height 25px
					color $white
					border none
					background $black
					border-bottom 4px solid #333
					border-radius 8px
					cursor pointer

			.locator-results
				display none

		.testimonals-holder
			float left
			width 280px
			padding 0
			margin 0 0 0 20px

			.testimonals
				float left
				width 280px
				padding 0
				margin 0 0 60px 0

				.testimonal
					float left
					width 240px
					padding 28px 20px 28px 20px
					margin 0 0 14px 0
					background $white
					border-bottom 4px solid #333
					border-radius 12px

					p
						float left
						width 100%
						padding 0
						margin 0 0 24px 0
						font-style italic
						font-weight 600
						font-size 19px
						line-height 31px
						color $black
						text-align center
						text-shadow $white-shadow

					h2
						float left
						width 100%
						padding 0
						font-weight 600
						font-size 23px
						line-height 23px
						margin 0 0 14px 0
						text-align center
						text-shadow $white-shadow

					h3
						float left
						width 100%
						padding 0
						margin 0
						font-style italic
						font-weight 500
						font-size 19px
						line-height 19px
						text-align center
						text-shadow $white-shadow

				.testimonal.hide
					display none