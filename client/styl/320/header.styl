header
	float left
	width 100%
	top 0
	padding 0
	margin 0
	background $header
	border-bottom $border-header

	.header
		position relative
		width 280px
		height 138px
		padding 0
		margin 0 auto

		.brand
			float left
			width 280px
			height 100%
			padding 0
			margin 0

			.logo
				float left
				width 278px
				height 128px
				padding 10px 0 0 0
				margin 0
				font-size 14px
				line-height 50px
				// background #d1d1d1
				border 1px dotted $black
				border-top none
				border-bottom none
				text-align center

				.mobile-navigation-button
					float left
					width 258px
					padding 10px 0 10px 0
					margin 60px 0 10px 10px
					background $card
					border $border
					border-radius 4px

					h5
						float left
						width 258px
						font-size 22px
						line-height 24px
						text-align center
						text-shadow $white-shadow

		nav
			display none
